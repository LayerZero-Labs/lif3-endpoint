{
  "name": "@layerzerolabs/ops-core",
  "version": "2.3.38",
  "license": "BUSL-1.1",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/**/*"
  ],
  "scripts": {
    "build": "$npm_execpath clean-prebuild && $npm_execpath build-ts",
    "build-ts": "$npm_execpath tsc --noEmit && $npm_execpath tsup",
    "clean": "$npm_execpath clean-prebuild && rimraf -g tsconfig.tsbuildinfo builtins client types utils wireable '*.d.ts' '*.map' '!(jest.config).js' .turbo",
    "clean-prebuild": "rimraf dist",
    "ops": "cd fixture-projects/counter && $npm_execpath ts-node bootstrap.ts",
    "test": "$npm_execpath jest ./**/*.test.ts"
  },
  "dependencies": {
    "@layerzerolabs/lz-definitions": "^2.3.39",
    "@layerzerolabs/lz-utilities": "^2.3.39",
    "@solana/web3.js": "^1.92.1",
    "aptos": "^1.20.0",
    "bip39": "^3.1.0",
    "commander": "^12.1.0",
    "ed25519-hd-key": "^1.3.0",
    "ethers": "^5.7.2",
    "find-up": "^5.0.0",
    "lodash": "^4.17.21",
    "stacktrace-parser": "^0.1.10",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@layerzerolabs/tsup-config-next": "^2.3.39",
    "@layerzerolabs/typescript-config-next": "^2.3.39",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.10",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.5",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.1",
    "typescript": "~5.2.2"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
